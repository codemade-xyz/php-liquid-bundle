{
	"name": "codemade-xyz/php-liquid-bundle",
	"type": "symfony-bundle",
	"description": "Symfony LiquidBundle template engine for PHP",
	"keywords": ["liquid", "template"],
	"homepage": "https://github.com/codemade-xyz/php-liquid-bundle",
    "license": "MIT",
    "authors": [
        {
			"name": "Andrew Koen",
			"email": "koen@codemade.xyz"
		}
    ],
    "require": {
		"php": ">= 7.1",
		"symfony/config": "^4.3",
		"symfony/http-kernel": "^4.3",
		"liquid/liquid": "^1.4",
		"symfony/templating": "^4.3"
	},
	"require-dev": {
		"phpunit/phpunit": "<6.0",
		"symfony/console": "^4.3",
		"symfony/dependency-injection": "^4.3"
	},
	"autoload": {
		"psr-4": {
			"CodeMade\\LiquidBundle\\": ""
		},
		"exclude-from-classmap": [
			"/Tests/"
		]
	}
}
